{
  "version": 8,
  "name": "OpenStreetMap Mapnik raster tiles (Default)",
  "metadata": {
    "mapbox:autocomposite": true
  },
  "glyphs": "https://raw.githubusercontent.com/orangemug/font-glyphs/gh-pages/glyphs/{fontstack}/{range}.pbf",
  "sources": {
    "osm-mapnik": {
      "minzoom": 0,
      "maxzoom": 17,
      "type": "raster",
      "tiles": ["/tiles/{z}/{x}/{y}.png"],
      "tileSize": 256,
      "attribution": "Basemap data <a href='https://www.osm.org' target=_blank>Â©  OpenStreetMap contributors</a>"
    }
  },
  "layers": [
    {
      "id": "background",
      "type": "background",
      "paint": {
        "background-color": "rgba(0,0,0,0)"
      }
    },
    {
      "id": "osm-mapnik",
      "type": "raster",
      "source": "osm-mapnik"
    }
  ],
  "owner": "OpenStreetMap India"
}
